{
  "name": "personalized-health-wellness-companion",
  "version": "1.0.0",
  "description": "AI-driven personalized health and wellness platform with biometric integration",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.2",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.4",
    "axios": "^1.5.0",
    "openai": "^4.8.0",
    "@tensorflow/tfjs-node": "^4.10.0",
    "ml-matrix": "^6.10.7",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "keywords": [
    "health",
    "wellness",
    "biometric",
    "ai",
    "fitness",
    "nutrition",
    "mental-health"
